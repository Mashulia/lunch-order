    "node-telegram-bot-api": "^0.64.0",
    "nodemailer": "^6.9.11",
    "nodemon": "^3.0.3",
    "python-shell": "^5.0.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.2.0",
    "sequelize": "^6.35.1",
    "sequelize-typescript": "^2.1.6",
    "swagger-ui-express": "^5.0.0",
    "telegraf": "^4.16.3",
    "telegraf-session-local": "^2.1.1",
    "typeorm": "^0.3.17",
    "xlsx": "^0.18.5",
    "xlsx-populate": "^1.21.0"
